  "dependencies": {
    "@material-ui/icons": "^4.11.3",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.0",
    "chai": "^4.3.6",
    "concurrently": "3.5.1",
    "config": "^3.3.7",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
